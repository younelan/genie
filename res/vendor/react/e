# Create vendor directories
mkdir -p /var/www/html/genie/res/vendor/bootstrap/css
mkdir -p /var/www/html/genie/res/vendor/react-bootstrap/dist

# Download Bootstrap CSS
wget https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css -O /var/www/html/genie/res/vendor/bootstrap/css/bootstrap.min.css

# Download React-Bootstrap
wget https://cdn.jsdelivr.net/npm/react-bootstrap@2.0.0/dist/react-bootstrap.js -O /var/www/html/genie/res/vendor/react-bootstrap/dist/react-bootstrap.js
