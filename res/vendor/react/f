# Create directories if they don't exist
mkdir -p /var/www/html/genie/res/vendor/react-bootstrap/dist
mkdir -p /var/www/html/genie/res/vendor/bootstrap/css

# Download updated files
wget https://cdn.jsdelivr.net/npm/react-bootstrap@2.8.0/dist/react-bootstrap.bundle.min.js -O /var/www/html/genie/res/vendor/react-bootstrap/dist/react-bootstrap.bundle.min.js
wget https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css -O /var/www/html/genie/res/vendor/bootstrap/css/bootstrap.min.css

